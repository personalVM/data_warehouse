config {
  type: "table",
  schema: "silver",
  name: "df_all"
}

CREATE OR REPLACE TABLE `personalvm1.silver.df_all` AS
SELECT 
  a.cd_micro,
  b.brazilians, 
  b.emig_total,
  c.exports
FROM ${ref('bronze', 'df_locations_micro')} AS a
INNER JOIN ${ref('bronze', 'df_emigrants_micro')} AS b ON a.cd_micro = b.cd_micro
INNER JOIN ${ref('bronze', 'df_exports_micro')} AS c ON a.cd_micro = c.cd_micro
